{
  "author": "Tim Osipchik",
  "license": "MIT",
  "name": "osprey-js",
  "version": "0.0.5",
  "description": "modern fast and lightweight backend framework",
  "files": [
    "build"
  ],
  "exports": {
    "./app": {
      "import": {
        "types": "./build/app.d.ts",
        "default": "./build/app.js"
      }
    },
    "./middleware": "./build/Middleware/index.js",
    "./utils/*.js": "./build/utils/*/index.js",
    "./utils/*": "./build/utils/*/index.js",
    "./Routing/decorators": {
      "import": {
        "types": "./build/Routing/decorators/index.d.ts",
        "default": "./build/Routing/decorators/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf ./build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@parisholley/road-runner": "^1.1.8",
    "dotenv": "^16.0.3",
    "http-parser-js": "^0.5.8"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "rimraf": "^4.1.2",
    "tsc-alias": "^1.8.2",
    "tslib": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Osipchik/osprey-js.git"
  },
  "bugs": {
    "url": "https://github.com/Osipchik/Lark/issues"
  },
  "homepage": "https://github.com/Osipchik/Lark#readme",
  "keywords": [
    "api",
    "backend",
    "js",
    "rest",
    "node",
    "express"
  ]
}
